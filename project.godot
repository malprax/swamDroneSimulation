; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="SwarmDroneSim"
run/main_scene="res://Main.tscn"
config/features=PackedStringArray("4.2")
config/icon="res://assets/drone_icon.png"

[display]

window/stretch/mode="canvas_items"
window/stretch/aspect="expand"
window/size/width=1280
window/size/height=720

[editor]

export/convert_text_resources_to_binary_on_export=false

[rendering]

environment/defaults/default_clear_color=Color(0.1, 0.1, 0.12, 1)
anti_aliasing/quality/msaa=2
